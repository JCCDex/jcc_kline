{
  "presets": [
    [
      "@babel/env",
      {
        "targets": {
          "browsers": ["> 1%", "last 2 versions", "not ie <= 8"]
        }
      }
    ]
  ],
  "plugins": [
    "transform-vue-jsx",
    "@babel/plugin-transform-runtime",
    [
      "import",
      {
        "libraryName": "node",
        "style": true
      }
    ],
    ["transform-modules"]
  ],
  "env": {
    "test": {
      "presets": ["@babel/env"],
      "plugins": [
        "transform-vue-jsx",
        "@babel/plugin-transform-modules-commonjs",
        "dynamic-import-node"
      ]
    }
  }
}
